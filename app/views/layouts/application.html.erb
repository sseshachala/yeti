<!DOCTYPE html>
<html>
<head>
  <title>Yeti</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
<style type="text/css"><!--
div.show { display: block }
div.hide { display: none; }
--></style>
<div class="container">
<%= render 'layouts/header' %>
<section class="round">
<% flash.each do |key, value| %>
  <div class="flash <%= key %>"><%= value %></div>
<% end  %>
<%= yield %>
</section>
<%= render 'layouts/footer' %>
<div>
</body>
</html>
